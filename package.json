{
  "name": "backend-form",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rm -rf ./dist && ./node_modules/.bin/tsc",
    "dev": "yarn build && ENV=dev nodemon ./dist/server.js",
    "serve:dev": "yarn build && ENV=dev node serve_dev.js",
    "start": "yarn build && node ./dist/server.js",
    "test": "yarn build && ENV=test ./node_modules/.bin/cucumber-js test/**/**/*.feature --require ./dist/test/**/*.steps.js --require ./dist/test/*.steps.js --require-module ts-node/register --exit"
  },
  "dependencies": {
    "@types/chai": "^4.2.7",
    "@types/cookie-parser": "^1.4.2",
    "@types/cucumber": "^6.0.0",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "@types/request-promise": "^4.1.45",
    "@types/uuid": "^3.4.6",
    "chai": "^4.2.0",
    "cookie-parser": "~1.4.4",
    "cucumber": "^6.0.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "ngrok": "^3.2.7",
    "pg": "^7.17.1",
    "prettier": "^1.19.1",
    "reflect-metadata": "^0.1.13",
    "request-promise": "^4.2.5",
    "ts-node": "^8.6.2",
    "typeorm": "^0.2.25",
    "typescript": "^3.7.5",
    "uuid": "^3.4.0"
  }
}
